{
  "name": "react-ts-tpl",
  "version": "0.0.1",
  "description": "react-ts-tpl",
  "private": true,
  "type": "module",
  "author": "jsjzh <kimimi_king@163.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development vite --port 8080 --open --mode development",
    "build:dev": "npm run build:development",
    "build:prepub": "npm run build:staging",
    "build:prod": "npm run build:production",
    "build:development": "tsc && NODE_ENV=development vite build --mode development",
    "build:staging": "tsc && NODE_ENV=staging vite build --mode staging",
    "build:production": "tsc && NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --fix",
    "format": "prettier --write --parser typescript \"**/*.[tj]s?(x)\" --config prettier.config.cjs",
    "format:check": "prettier --check --parser typescript \"**/*.[tj]s?(x)\" --config prettier.config.cjs"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/icons": "^5.5.1",
    "ahooks": "^3.8.1",
    "antd": "^5.20.6",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "jsonp": "^0.2.1",
    "localforage": "^1.10.0",
    "query-string": "^9.1.0",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "styled-components": "^6.1.13",
    "use-immer": "^0.9.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/jsonp": "^0.2.3",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.1",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-react": "^7.36.1",
    "less": "^4.2.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.6"
  }
}
