{
  "name": "react-ts-tpl",
  "version": "0.0.1",
  "description": "react-ts-tpl",
  "private": true,
  "type": "module",
  "author": "jsjzh <kimimi_king@163.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --port 8080 --host local.dasouche-inc.net --open",
    "build:dev": "tsc && vite build --mode dev",
    "build:test": "tsc && vite build --mode test",
    "build:staging": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint \"./**/*.{ts,tsx}\" --cache --fix",
    "format": "prettier --write --cache --parser typescript \"**/*.[tj]s?(x)\" --config prettier.config.cjs",
    "format:check": "prettier --check --cache --parser typescript \"**/*.[tj]s?(x)\" --config prettier.config.cjs"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/icons": "^5.3.6",
    "ahooks": "^3.7.11",
    "antd": "^5.16.5",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "decimal.js": "^10.4.3",
    "immer": "^10.1.1",
    "js-base64": "^3.7.7",
    "jsonp": "^0.2.1",
    "localforage": "^1.10.0",
    "query-string": "^9.0.0",
    "ramda": "^0.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "styled-components": "^6.1.8",
    "use-immer": "^0.9.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/jsonp": "^0.2.3",
    "@types/node": "^20.12.7",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-react": "^7.34.1",
    "less": "^4.2.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}
